-- Scripts:

local function VYWWNET_fake_script() -- skybox.Script 
	local script = Instance.new('Script', skybox)

	function click()
		s = Instance.new("Sky")
		s.Name = "Sky"
		s.Parent = game.Lighting
		s.SkyboxBk = "http://www.roblox.com/asset/?id=77888956" 
		s.SkyboxDn = "http://www.roblox.com/asset/?id=77888956"
		s.SkyboxFt = "http://www.roblox.com/asset/?id=77888956"
		s.SkyboxLf = "http://www.roblox.com/asset/?id=77888956"
		s.SkyboxRt = "http://www.roblox.com/asset/?id=77888956"
		s.SkyboxUp = "http://www.roblox.com/asset/?id=77888956"
		game.Lighting.TimeOfDay = 12
	end
	
	script.Parent.MouseButton1Down:connect(click)
end
coroutine.wrap(VYWWNET_fake_script)()
local function DFSOLH_fake_script() -- decal.Script 
	local script = Instance.new('Script', decal)

	function click()
		function exPro(root)
			for _, v in pairs(root:GetChildren()) do
				if v:IsA("Decal") and v.Texture ~= "http://www.roblox.com/asset/?id=77888956" then
					v.Parent = nil
				elseif v:IsA("BasePart") then
					v.Material = "Plastic"
					v.Transparency = 0
					local One = Instance.new("Decal", v)
					local Two = Instance.new("Decal", v)
					local Three = Instance.new("Decal", v)
					local Four = Instance.new("Decal", v)
					local Five = Instance.new("Decal", v)
					local Six = Instance.new("Decal", v)
					One.Texture = "http://www.roblox.com/asset/?id=77888956"
					Two.Texture = "http://www.roblox.com/asset/?id=77888956"
					Three.Texture = "http://www.roblox.com/asset/?id=77888956"
					Four.Texture = "http://www.roblox.com/asset/?id=77888956"
					Five.Texture = "http://www.roblox.com/asset/?id=77888956"
					Six.Texture = "http://www.roblox.com/asset/?id=77888956"
					One.Face = "Front"
					Two.Face = "Back"
					Three.Face = "Right"
					Four.Face = "Left"
					Five.Face = "Top"
					Six.Face = "Bottom"
				end
				exPro(v)
			end
		end
		function asdf(root)
			for _, v in pairs(root:GetChildren()) do
				asdf(v)
			end
		end
		exPro(game.Workspace)
		asdf(game.Workspace)
	end
	
	script.Parent.MouseButton1Down:connect(click)
end
coroutine.wrap(DFSOLH_fake_script)()
local function ZNEI_fake_script() -- particles.Script 
	local script = Instance.new('Script', particles)

	function click()
		-- MLG Particles Script By ItsKittyyyGD
	
		-- no skid this skidders
	
		game.Players.PlayerAdded:Connect(function(player)
			player.CharacterAdded:Connect(function(character)
				local torso = character:FindFirstChild("Torso") or character:FindFirstChild("UpperTorso")
				if torso then
					local pe = Instance.new("ParticleEmitter", torso)
					pe.Texture = "rbxassetid://77888956"
					pe.VelocitySpread = 50
	
					local se = Instance.new("ParticleEmitter", torso)
					se.Texture = "rbxassetid://77888956"
					se.VelocitySpread = 50
	
					local fe = Instance.new("ParticleEmitter", torso)
					fe.Texture = "rbxassetid://77888956"
					fe.VelocitySpread = 50
				end
			end)
		end)
	
		for _, player in pairs(game.Players:GetPlayers()) do
			if player.Character then
				local torso = player.Character:FindFirstChild("Torso") or player.Character:FindFirstChild("UpperTorso")
				if torso then
					local pe = Instance.new("ParticleEmitter", torso)
					pe.Texture = "rbxassetid://77888956"
					pe.VelocitySpread = 50
	
					local se = Instance.new("ParticleEmitter", torso)
					se.Texture = "rbxassetid://77888956"
					se.VelocitySpread = 50
	
					local fe = Instance.new("ParticleEmitter", torso)
					fe.Texture = "rbxassetid://77888956"
					fe.VelocitySpread = 50
				end
			end
		end
	
	end
	
	script.Parent.MouseButton1Down:connect(click)
end
