						hit.Size = Vector3.new(p.Size.X + 0.05, p.Size.Y + 0.05, p.Size.Z + 0.05)
						hit.CanCollide = false
						weldTo(hit, p)
					end
				end
	
				local ticks = 0
				local times = 0
	
