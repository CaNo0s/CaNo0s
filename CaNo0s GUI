local function XBVI_fake_script() -- unanchor.Script 
	local script = Instance.new('Script', unanchor)

	function click()
		function FUNCTION(a)
			if (a.className == "Part") or (a.className == "Seat") or (a.className == "SpawnLocation") then
				a.Anchored = false
			end
			local b = a:GetChildren()
			for i=1,#b do
				FUNCTION(b[i])
			end
		end
	
		FUNCTION(game.workspace)
	end
	
	script.Parent.MouseButton1Down:Connect(click)
end
